pipeline{
  agent any

  stages{
    stage('unittests'){
      steps{
        sh 'ant -f test.xml -v'
        junit 'reports/result.xml'
      }
    }
    stage('build'){
      steps{
      sh 'ant -f build.xml -v'
      }
    }
  }
  post {
      always {
          archiveArtifacts artifacts: '**/*.jar', fingerprint: true
      }
  }
}
